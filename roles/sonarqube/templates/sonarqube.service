[Unit]
Description=SonarQube service
After=syslog.target network.target

[Service]
Type=forking

ExecStart=/sonar/sonarqube-10.7.0.96327/bin/linux-x86-64 start
ExecStop=/sonar/sonarqube-10.7.0.96327/bin/linux-x86-64 stop

User=vagrant
Group=vagrant
Restart=always

[Install]
WantedBy=multi-user.target
